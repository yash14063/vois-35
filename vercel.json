{
  "routes": [
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/(.*\\.js)$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*\\.css)$",
      "dest": "/$1"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
